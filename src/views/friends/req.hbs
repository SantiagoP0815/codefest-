    <div class="row">
        {{#each fReq}}
        <div class="col-md-3">
            <div class="card text-center">
                <div class="card-body">
                    <a href="/posts/show/{{id_post}}" target="_blank">
                        <h3 class="card-title text-uppercase">
                            {{p_title}}
                        </h3>
                    </a>
                    <p class="text-muted">
                        {{timeago created_at}}
                    </p>
                    <a class="btn btn-danger" href="/friends/delete/{{id_friends}}">
                        Accept
                    </a>
                    <a class="btn btn-secondary" href="/friends/accept{{id_friends}}">
                        Discard
                    </a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>